{
  "presets": [
    [
      "@babel/preset-env",
      "@babel/preset-typescript",
      {
        "useBuiltIns": "entry"
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "root": [
          "./src"
        ],
        "alias": {
          "features": "./src/redux/features",
          "theme": "./src/theme",
          "api": "./src/redux/api",
          "testHelpers": "./src/testHelpers"
        }
      }
    ],
    "@babel/plugin-proposal-class-properties"
  ]
}